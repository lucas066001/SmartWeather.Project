{
    "name": "mqtt-sink-tostation-connector",
    "config": {
        "confluent.topic.bootstrap.servers": "broker-1:9092,broker-2:9093,broker-3:9094",
        "connector.class": "io.confluent.connect.mqtt.MqttSinkConnector",
        "tasks.max": "1",
        "mqtt.server.uri": "tcp://hivemq-proxy:1883",
        "topics": "station.config.tostation",
        "value.converter": "org.apache.kafka.connect.storage.StringConverter",
        "key.converter": "org.apache.kafka.connect.storage.StringConverter",
        "mqtt.qos": "1",
        "mqtt.clean.session": "true",
        "mqtt.client.id": "kafka-mqtt-sink-tostation"
    }
}