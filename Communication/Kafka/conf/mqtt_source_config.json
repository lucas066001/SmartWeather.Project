{
    "name": "mqtt-source-toserver-connector",
    "config": {
        "confluent.topic.bootstrap.servers": "kafka-0:9092,kafka-1:9092,kafka-2:9092",
        "connector.class": "io.confluent.connect.mqtt.MqttSourceConnector",
        "tasks.max": "1",
        "mqtt.server.uri": "tcp://hivemq-proxy:1883",
        "mqtt.topics": "station/+/measure_point/+/toserver",
        "kafka.topic": "smartweather.measure.data",
        "value.converter": "org.apache.kafka.connect.converters.ByteArrayConverter",
        "key.converter": "org.apache.kafka.connect.storage.StringConverter",
        "mqtt.qos": "1",
        "mqtt.clean.session": "true",
        "mqtt.client.id": "kafka-mqtt-source-connector"
    }
}