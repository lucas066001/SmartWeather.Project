{
    "name": "mqtt-source-toserver-connector",
    "config": {
        "confluent.topic.bootstrap.servers": "broker-1:9092,broker-2:9093,broker-3:9094",
        "connector.class": "io.confluent.connect.mqtt.MqttSourceConnector",
        "tasks.max": "1",
        "mqtt.server.uri": "tcp://hivemq-proxy:1883",
        "mqtt.topics": "station.config.toserver",
        "kafka.topic": "station.config.toserver",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "mqtt.qos": "1",
        "mqtt.clean.session": "true",
        "mqtt.client.id": "kafka-mqtt-source-toserver"
    }
}